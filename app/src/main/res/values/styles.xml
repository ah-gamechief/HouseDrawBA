<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="a">
        <item name="android:gravity">bottom|center_vertical|center_horizontal|center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="view_style">
        <item name="android:layout_width">1.5dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@color/line_color</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintHeight_percent">0.4</item>
    </style>
